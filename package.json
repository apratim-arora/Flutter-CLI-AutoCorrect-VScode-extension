{
  "name": "flutter-cli-autocorrect",
  "displayName": "Flutter CLI Autocorrect",
  "description": "Automatically corrects mistyped Flutter and Dart CLI commands in the terminal, saving time and frustration.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:flutter-cli-autocorrect.fixCommand"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-cli-autocorrect.fixCommand",
        "title": "Fix: Flutter/Dart Terminal Input"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/MY_GITHUB_USERNAME"
  },
  "badges": [
    {
      "label": "Buy Me a Coffee",
      "message": "Support Me",
      "color": "yellow",
      "url": "https://www.buymeacoffee.com/MY_USERNAME"
    }
  ]
}
